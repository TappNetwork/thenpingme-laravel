{
    "name": "thenpingme/laravel",
    "description": "Zero config scheduled task monitoring for Laravel",
    "keywords": [
        "thenpingme",
        "laravel"
    ],
    "homepage": "https://github.com/thenpingme/laravel",
    "license": "GPL-2.0-or-later",
    "type": "library",
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0|^7.2.5",
        "kitetail/zttp": "^0.4.0",
        "laravel/framework": "^6.2|^7.0",
        "nunomaduro/laravel-console-task": "^1.4",
        "sixlive/dotenv-editor": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "orchestra/testbench": "4.* || 5.*",
        "phpunit/phpunit": "^6.0 || 7.0 || 8.0",
        "sempro/phpunit-pretty-print": "^1.1",
        "spatie/phpunit-watcher": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "Thenpingme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thenpingme\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --color=always",
        "test-coverage": "vendor/bin/phpunit --color=always --coverage-html coverage",
        "test-watch": "vendor/bin/phpunit-watcher watch"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thenpingme\\ThenpingmeServiceProvider"
            ],
            "aliases": {
                "Laravel": "Thenpingme\\Facades\\Thenpingme"
            }
        }
    }
}
